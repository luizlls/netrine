//! test: unary precedence
-2 ^ 3
//! expect:
BINARY 0..6
  UNARY 0..2
    OPERATOR(-) 0..1
    INTEGER(2) 1..2
  OPERATOR(^) 3..4
  INTEGER(3) 5..6


//! test: multiple lines
1 + 2
3 * 4
5 / 6
//! expect:
BINARY 0..5
  INTEGER(1) 0..1
  OPERATOR(+) 2..3
  INTEGER(2) 4..5
BINARY 6..11
  INTEGER(3) 6..7
  OPERATOR(*) 8..9
  INTEGER(4) 10..11
BINARY 12..17
  INTEGER(5) 12..13
  OPERATOR(/) 14..15
  INTEGER(6) 16..17
