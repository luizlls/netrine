// test: basic
1 + 1
--
BINARY 0..5
  INTEGER `1` 0..1
  OPERATOR `+` 2..3
  INTEGER `1` 4..5


// test: integers and floats
3.14 ^ 2
--
BINARY 0..8
  NUMBER `3.14` 0..4
  OPERATOR `^` 5..6
  INTEGER `2` 7..8


// test: operator precedence
1 + 2 - 3 * 4 / 5 ^ 2 ^ 2
--
BINARY 0..25
  BINARY 0..5
    INTEGER `1` 0..1
    OPERATOR `+` 2..3
    INTEGER `2` 4..5
  OPERATOR `-` 6..7
  BINARY 8..25
    BINARY 8..13
      INTEGER `3` 8..9
      OPERATOR `*` 10..11
      INTEGER `4` 12..13
    OPERATOR `/` 14..15
    BINARY 16..25
      INTEGER `5` 16..17
      OPERATOR `^` 18..19
      BINARY 20..25
        INTEGER `2` 20..21
        OPERATOR `^` 22..23
        INTEGER `2` 24..25
