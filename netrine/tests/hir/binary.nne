//! test: unary precedence
-2 ^ 3
//! expect:
BINARY: integer
  UNARY: integer
    OPERATOR(neg)
    INTEGER(2): integer
  OPERATOR(pow)
  INTEGER(3): integer


//! test: multiple lines
1 + 2
3 * 4.2
5 / 6
//! expect:
BINARY: integer
  INTEGER(1): integer
  OPERATOR(add)
  INTEGER(2): integer
BINARY: number
  INTEGER(3): integer
  OPERATOR(mul)
  NUMBER(4.2): number
BINARY: number
  INTEGER(5): integer
  OPERATOR(div)
  INTEGER(6): integer
