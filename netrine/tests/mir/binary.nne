//! test: basic arithmetic
1 + 1
//! expect:
v0: integer = 1
v1: integer = 1
v2: integer = add v0 v1


//! test: integers and floats
3.14 * 2
//! expect:
v0: number = 3.14
v1: integer = 2
v2: number = mul v0 v1


//! test: parenthesis
5 * (2 + 3)
//! expect:
v0: integer = 5
v1: integer = 2
v2: integer = 3
v3: integer = add v1 v2
v4: integer = mul v0 v3


//! test: operator precedence
1 + 2 - 3 * 4 / 5 + 2.54
//! expect:
v0: integer = 1
v1: integer = 2
v2: integer = add v0 v1
v3: integer = 3
v4: integer = 4
v5: integer = mul v3 v4
v6: integer = 5
v7: number = div v5 v6
v8: number = sub v2 v7
v9: number = 2.54
v10: number = add v8 v9

