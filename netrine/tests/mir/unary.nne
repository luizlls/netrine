//! test: negative
-10
//! expect:
v0: integer = 10
v1: integer = neg v0


//! test: negative float
-3.14
//! expect:
v0: number = 3.14
v1: number = neg v0


//! test: chained
--1
//! expect:
v0: integer = 1
v1: integer = neg v0
